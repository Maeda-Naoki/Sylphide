{
  "name": "RustDevContainer",
  "dockerComposeFile": "../Docker-compose.yml",
  "service": "rust",
  "workspaceFolder": "/home/developer/DevApp",
  "settings": {
    "terminal.integrated.profiles.linux": {
      "Bash": {
        "path": "/bin/bash"
      }
    },
    // VS Code don't watch files under ./target
    "files.watcherExclude": {
      "**/target/**": true
    }
  },
}
